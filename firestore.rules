rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match / {Categories}/{catgories} {
      allow read : if true;
    }
    match / {WordsToFind} / {word} {
    	allow read : if true;
    }
    match / {Players} /{player} {
    	allow read : if true;
      allow write : if true;
    }
    
  }
}